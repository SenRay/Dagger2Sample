<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.demo.lizejun.dagger2sample">

    <application
        android:name=".chapter5.App"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".chapter1.RepositoryActivity" />
        <activity android:name=".chapter4.activity.ScopeActivity" />
        <activity android:name=".chapter2.QualifierActivity" />
        <activity android:name=".chapter3.ComponentActivity" />
        <activity android:name=".chapter5.HomeActivity"></activity>
    </application>

</manifest>